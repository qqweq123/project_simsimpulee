{
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/maintenance"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "x-middleware-prefetch",
                    "value": "1"
                },
                {
                    "key": "Strict-Transport-Security",
                    "value": "max-age=31536000; includeSubDomains; preload"
                }
            ]
        }
    ]
}